{
  "name": "resume-builder",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push --force",
    "db:init": "dotenv -e .env -- sh -c 'psql \"$POSTGRES_URL\" -f ./db/init-db.sql'",
    "db:run": "docker-compose up -d postgres",
    "remove-comments": "node remove-comments.cjs && npm run format",
    "check:unused": "knip",
    "fix:unused": "knip --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint --max-warnings 0 && tsc --noEmit",
    "lint:fix": "next lint --fix --max-warnings 0"
  },
  "dependencies": {
    "@google/genai": "^1.6.0",
    "@hono/zod-validator": "^0.4.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.2",
    "@react-pdf/renderer": "^4.3.0",
    "@smastrom/react-rating": "^1.5.0",
    "@tanstack/react-query": "^5.59.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.34.1",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.6.4",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.11.2",
    "lucide-react": "^0.453.0",
    "next": "15.5.2",
    "next-intl": "^4.3.7",
    "next-themes": "^0.4.6",
    "pg": "^8.16.0",
    "puppeteer": "^24.17.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-simple-wysiwyg": "^3.4.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@next/eslint-plugin-next": "^15.5.2",
    "@types/node": "^20",
    "@types/pg": "^8.15.4",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.25.0",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "knip": "^5.61.2",
    "postcss": "^8",
    "prettier": "^3.6.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.35.0"
  },
  "packageManager": "yarn@4.9.1",
  "overrides": {
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6"
  }
}
